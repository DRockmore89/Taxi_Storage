Feel free to use these for server testing at: http://localhost:3001/graphql



mutation addCustomer {
  addCustomer (
  firstName: "Beth", 
  lastName: "Smith", 
  email: "BS123@gmail.com", 
  password: "password123", 
  address: "123 Happy Street", 
  city: "Atlanta", 
  state: "Georgia", 
  zip: "30350", phone: "404-555-5555") {
    token,
    customer {
      _id
      firstName
      lastName
      email
      password
      address	
      city
      state
      zip
      phone
  	} 
  }
}




mutation login {
  login (email: "BS123@gmail.com", password: "password123" ) {
    token,
    customer {
      _id
      email
    }
  }
}



query getCustomers {
  customers {
    _id
    firstName
    lastName
    email
    password
    address
    city
    state
    zip
    phone
    boxes {
      _id
      boxSize
      sendToCustomer
      getFromCustomer
    }
  }
}

query getCustomerById {
  customer (customerId: "60f4be1de5786e44b816d88d") {
    _id
    firstName
    lastName
    email
    password
    address
    city
    state
    zip
    phone
      boxes {
      _id
      boxSize
      sendToCustomer
      getFromCustomer
    }
  }
}
